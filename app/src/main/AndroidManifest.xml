<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.otopreneur.otopreneurbengkel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_bengkel"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".Order.OrderAcceptActivity" android:parentActivityName=".Order.DetailOrderActivity"></activity>
        <activity android:name=".Order.DetailOrderActivity" android:parentActivityName=".Dashboard.OrderActivity" />
        <activity android:name=".History_Order.DetailHistoryActivity" android:parentActivityName=".Dashboard.HistoryOrderActivity" />
        <activity android:name=".Dashboard.SettingsActivity" android:parentActivityName=".DashboardActivity" />
        <activity android:name=".Dashboard.OrderActivity" android:parentActivityName=".DashboardActivity" />
        <activity android:name=".Dashboard.HistoryOrderActivity" android:parentActivityName=".Dashboard.OrderActivity" />
        <activity android:name=".Dashboard.DaftarServiceActivity" android:parentActivityName=".Dashboard.OrderActivity"/>
        <activity android:name=".Daftar_Service.DetailServiceActivity" android:parentActivityName=".Dashboard.OrderActivity" />
        <activity android:name=".Daftar_Service.AddServiceActivity" android:parentActivityName=".Dashboard.DaftarServiceActivity"/>
        <activity android:name=".IsiInput" android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity"/>
        </activity>
        <activity android:name=".DashboardActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>