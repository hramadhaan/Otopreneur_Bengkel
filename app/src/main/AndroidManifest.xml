<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.otoprenuer.vendor_otopreneur">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.otoprenuer.vendor_otopreneur.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_bengkel_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_bengkel"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name="com.otoprenuer.vendor_otopreneur.Order.OrderAcceptActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Order.DetailOrderActivity"></activity>
        <activity android:name="com.otoprenuer.vendor_otopreneur.Order.DetailOrderActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Dashboard.OrderActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.History_Order.DetailHistoryActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Dashboard.HistoryOrderActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.Dashboard.SettingsActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.DashboardActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.Dashboard.OrderActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.DashboardActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.Dashboard.HistoryOrderActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Dashboard.OrderActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.Dashboard.DaftarServiceActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Dashboard.OrderActivity"/>
        <activity android:name="com.otoprenuer.vendor_otopreneur.Daftar_Service.DetailServiceActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Dashboard.OrderActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.Daftar_Service.AddServiceActivity" android:parentActivityName="com.otoprenuer.vendor_otopreneur.Dashboard.DaftarServiceActivity"/>
        <activity android:name="com.otoprenuer.vendor_otopreneur.IsiInput" android:parentActivityName="com.otoprenuer.vendor_otopreneur.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.otoprenuer.vendor_otopreneur.LoginActivity"/>
        </activity>
        <activity android:name="com.otoprenuer.vendor_otopreneur.DashboardActivity" />
        <activity android:name="com.otoprenuer.vendor_otopreneur.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>